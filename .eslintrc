{
   "parser": "babel-eslint",
   "env": {
      "es6": true,
      "browser": true,
      "node": true
   },
   "rules": {
      "semi": "off",
      "arrow-parens": "off",
      "comma-dangle": "off",
      "require-jsdoc": "off",
      "indent": "off",
      "linebreak-style": "off",
      "eol-last": "off",
      "object-curly-spacing": "off"
   },
   "extends": [
      "eslint:recommended",
      "google"
   ]
}